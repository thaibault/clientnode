        "vendor": {
          "__evaluate__": "self.debug ? ['babel-polyfill', 'ua-parser-js'].concat(2 < self.givenCommandLineArguments.length && ['build:dll', 'serve', 'watch:dll'].includes(self.givenCommandLineArguments[2]) ? 'website-utilities' : []) : []"
