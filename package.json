{
  "name": "clientnode",
  "version": "2.0.0",
  "license": "CC-BY-3.0",
  "description": "upgrade to object orientated rock solid plugins",
  "homepage": "http://torben.website/clientNode",
  "documentationWebsite": {
    "languages": [
      "deDE",
      "enUS"
    ],
    "name": "clientNode",
    "tagline": "upgrade to object orientated rock solid plugins<!--deDE:Schreibe Objekt orientierte solide Plugins-->",
    "trackingCode": "UA-40192634-7"
  },
  "keywords": [
    "client",
    "tools",
    "node",
    "polymorph",
    "jquery"
  ],
  "files": [
    "index.js",
    "test.js",
    "test.compiled.js"
  ],
  "main": "index.compiled.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/clientNode"
  },
  "scripts": {
    "build": "webOptimizer build && npm run build:test",
    "build:alternatively": "babel --plugins transform-flow-strip-types,transform-runtime --presets env,stage-0 --source-maps inline --out-file index.compiled.js index.js && npm run build:test",
    "build:dll": "webOptimizer build:dll",
    "build:export": "webOptimizer build '{__reference__: \"export\"}' && npm run build:test",
    "build:export:compatible": "webOptimizer build '{__reference__: [\"export\", \"exportCompatible\"]}' && npm run build:test",
    "build:test": "babel --plugins transform-flow-strip-types,transform-runtime --presets env,stage-0 --source-maps inline --out-file test.compiled.js test.js",
    "clear": "webOptimizer clear",
    "document": "webOptimizer document",
    "lint": "webOptimizer check:type; webOptimizer lint",
    "prepare": "if ! npm run build; then echo You should rerun install procedure to resolve dependency cycle. && npm run buildAlternatively; fi",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "webOptimizer serve",
    "start": "npm run serve",
    "test": "nyc npm run test:plain",
    "test:browser": "webOptimizer test:browser",
    "test:plain": "webOptimizer test",
    "update": "npm version patch && npm publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch",
    "watch:dll": "webOptimizer build:dll --watch"
  },
  "peerDependencies": {
    "babel-runtime": "*",
    "jquery": "*",
    "qunit": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-proto-to-assign": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "coveralls": "^3.0.0",
    "documentation-website": "^1.0.106",
    "eslint": "^4.18.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-jsdoc": "^3.5.0",
    "flow-bin": "^0.66.0",
    "jquery": "^3.3.1",
    "jsdoc": "^3.5.3",
    "nyc": "^11.5.0",
    "qunit": "^2.5.0",
    "rimraf": "^2.6.1",
    "source-map-support": "^0.5.3",
    "weboptimizer": "^1.0.533",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2"
  },
  "webOptimizer": {
    "export": {
      "development": {
        "tool": false
      },
      "module": {
        "preprocessor": {
          "javaScript": {
            "options": {
              "plugins": {
                "__remove__": "transform-runtime"
              }
            }
          }
        }
      }
    },
    "exportCompatible": {
      "module": {
        "preprocessor": {
          "javaScript": {
            "options": {
              "plugins": {
                "__append__": "transform-proto-to-assign"
              },
              "presets": [
                "env",
                "stage-0"
              ]
            }
          }
        }
      }
    },
    "injection": {
      "internal": {
        "vendor": {
          "__evaluate__": "self.debug && 2 < self.givenCommandLineArguments.length && ['build:dll', 'serve', 'test:browser', 'watch:dll'].includes(self.givenCommandLineArguments[2]) ? ['babel-polyfill'].concat(self.givenCommandLineArguments[2] === 'serve' ? [] : ['script!qunit', 'weboptimizer/browserAPI']) : []"
        }
      }
    },
    "module": {
      "aliases": {
        "clientnode$": "./index",
        "jquery$": "jquery/dist/jquery.min"
      },
      "optimizer": {
        "babelMinify": {
          "module": {
            "guards": false,
            "mangle": false,
            "simplify": false
          }
        }
      },
      "skipParseRegularExpressions": {
        "__evaluate__": "/(?:^|\\/)(?:(?:babel-polyfill\\/dist\\/polyfill)|(?:jquery\\/dist\\/jquery)|(?:webpack\\/buildin\\/global)|(?:webpack\\/buildin\\/module))(?:\\.min)?\\.js(?:\\?.+|$)/"
      }
    }
  }
}
